#include<stdio.h>
void main()
{
    int a[10],i,n,p,ele;
    printf("enter the no of elements\n");
    scanf("%d",&n);
    printf("enter the elements of array\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the element to insert to insert\n");
    scanf("%d",&ele);
    printf("enter the pos of ele\n");
    scanf("%d",&p);
    for(i=n-1;i>=p-1;i--)
    {
        a[i+1]=a[i];
    }
    a[p]=ele;
    n++;
    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
